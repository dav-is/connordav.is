{
  "private": true,
  "name": "connordavis",
  "version": "0.1.0",
  "license": "MIT",
  "author": "Connor Davis",
  "scripts": {
    "dev": "next",
    "devT": "start-server-and-test dev http://localhost:3000 cy:open",
    "serve": "yarn export && yarn start",
    "start": "serve -l 3000 -n dist",
    "build": "next build",
    "export": "yarn build && next export -o dist",
    "test": "yarn lint && yarn cy:headless",
    "lint": "flow && eslint ./ *.js",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:headless": "start-server-and-test start http://localhost:3000 cy:run",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect && yarn snyk-protect",
    "now-build": "yarn export",
    "now-dev": "next"
  },
  "dependencies": {
    "next": "^9.1.1",
    "react": "^16.10.2",
    "react-dom": "^16.11.0",
    "snyk": "^1.237.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "cypress": "^3.4.1",
    "eslint": "^6.5.1",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-flow": "^1.0.1",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-cypress": "^2.7.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-standard": "^4.0.1",
    "flow-bin": "^0.109.0",
    "husky": "^3.0.9",
    "serve": "^11.2.0",
    "start-server-and-test": "^1.10.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "snyk": true
}
